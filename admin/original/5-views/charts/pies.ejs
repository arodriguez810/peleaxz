                            <%- CHART.circle("circle", scope, {}) %>


                        </div>
                    </div>

                </div>
                <div class="col-sm-6 col-md-6">
                    <div class="panel panel-flat">
                        <div class="panel-heading">
                            <h5 class="panel-title">Rose</h5>
