                                <%= scope %>.pie_data = [
                                    {value: 335, name: 'IE'},
                                    {value: 310, name: 'Opera'},
                                    {value: 234, name: 'Safari'},
                                    {value: 135, name: 'Firefox'},
                                    {value: 1548, name: 'Chrome'}
                                ];

                            </script>
                            <%- CHART.pie("pie", scope, {}) %>

                        </div>
                    </div>

                </div>
                <div class="col-sm-6 col-md-6">
                    <div class="panel panel-flat">
                        <div class="panel-heading">
                            <h5 class="panel-title">Circle</h5>
