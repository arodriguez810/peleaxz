                                <div class="chart has-fixed-height" style="width: 1000px" id="bar2"></div>
                            </div>
                            <script>
                                <%= scope %>.bar2_data = {
                                    legend: ['Internet Explorer', 'Opera', 'Safari', 'Firefox', 'Chrome'],
                                    left: ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday', 'Sunday'],
                                    data: [
                                        {
                                            name: 'Internet Explorer',
                                            type: 'bar',
                                            stack: 'Total',
                                            itemStyle: {
                                                normal: {
                                                    color: '#42A5F5',
                                                    label: {
                                                        show: true,
                                                        position: 'insideRight'
                                                    }
                                                },
                                                emphasis: {
                                                    color: '#42A5F5',
                                                    label: {
                                                        show: true
                                                    }
                                                }
