                                        },
                                        {
                                            name: 'Safari',
                                            type: 'bar',
                                            stack: 'Total',
                                            itemStyle: {
                                                normal: {
                                                    color: '#66bb6a',
                                                    label: {
                                                        show: true,
                                                        position: 'insideRight'
                                                    }
                                                },
                                                emphasis: {
                                                    color: '#66bb6a',
                                                    label: {
                                                        show: true
                                                    }
                                                }
                                            },
                                            data: [220, 182, 191, 234, 290, 330, 310]
                                        },
                                        {
                                            name: 'Firefox',
                                            type: 'bar',
                                            stack: 'Total',
