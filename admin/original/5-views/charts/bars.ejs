                    </div>

                </div>
                <div class="col-sm-12 col-md-12" ng-show="charts.allow(['Columns'])">
                    <div class="panel panel-flat">
                        <div class="panel-heading">
                            <h5 class="panel-title">Columns</h5>
                            <div class="heading-elements">
                            </div>
                        </div>

                        <div class="panel-body">
                            <div class="chart-container has-scroll">
                                <div class="chart has-fixed-height" style="width: 1000px" id="columns"></div>
                            </div>
                            <script>
                                var legendt = [];
                                legendt.push('Estatus 1');
                                legendt.push('Estatus 2');

                                var dataRereport = [];
                                dataRereport.push({
                                    name: 'Estatus 1',
                                    type: 'bar',
                                    data: [120]
                                });
                                dataRereport.push({
                                    name: 'Estatus 2',
                                    type: 'bar',
                                    data: [180]
                                });
                                <%= scope %>.columns_data = {
                                    legend: legendt,
                                    down: ['--'],
                                    data: dataRereport
                                }


                            </script>
                            <%- CHART.columns("columns", scope, {}) %>


                        </div>
                    </div>

                </div>
                <div class="col-sm-12 col-md-12" ng-show="charts.allow(['ColumnsStack'])">
                    <div class="panel panel-flat">
                        <div class="panel-heading">
                            <h5 class="panel-title">Columns Stack</h5>
                            <div class="heading-elements">
                            </div>
                        </div>

                        <div class="panel-body">
                            <div class="chart-container has-scroll">
                                <div class="chart has-fixed-height" style="width: 1000px" id="columnsstack"></div>
                            </div>
                            <script>
                                <%= scope %>.columnsstack_data = {
                                    legend: ['Internet Explorer', 'Opera', 'Safari', 'Firefox', 'Chrome'],
                                    down: ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday', 'Sunday'],
                                    data: [
                                        {
                                            name: 'Internet Explorer',
                                            type: 'bar',
                                            stack: 'Total',
                                            data: [120, 132, 101, 134, 490, 230, 210]
                                        },
                                        {
                                            name: 'Safari',
                                            type: 'bar',
                                            stack: 'Total',
                                            data: [150, 1232, 901, 154, 190, 330, 810]
                                        },
                                        {
                                            name: 'Firefox',
