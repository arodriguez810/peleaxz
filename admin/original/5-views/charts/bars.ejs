<div id="<%= scope %>" ng-controller="<%= scope %> as <%= scope %>">
    <div class="panel panel-flat">
        <%- include('../../7-plugins/templates/header/begin',{scope:scope}); %>
        <ul class="breadcrumb-elements visible-elements">
            <li dragonlanguage="">
                <a onclick="alert('Holaaaa')">
                    <i dragonlanguage="" title="MESSAGE.ic('mono.add')" class="icon-mail5"></i>Â 
                </a>
            </li>
        </ul>
        <%- include('../../7-plugins/templates/header/end',{scope:scope}); %>
        <div class="subcontent">
            <div class="row">
                <div class="col-sm-12 col-md-12" ng-show="charts.allow(['bar'])">
                    <div class="panel panel-flat">
                        <div class="panel-heading">
                            <h5 class="panel-title">Bar</h5>
                            <div class="heading-elements">
                            </div>
                        </div>

                        <div class="panel-body">
                            <div class="chart-container has-scroll">
                                <div class="chart has-fixed-height" style="width: 1000px" id="bar">

                                </div>
                            </div>
                            <script>
                                <%= scope %>.bar_data = {
                                    legend: ['Year 2013', 'Year 2014'],
                                    left: ['Germany', 'France', 'Spain', 'Netherlands', 'Belgium'],
                                    data: [
                                        {
                                            name: 'Year 2013',
                                            type: 'bar',
                                            itemStyle: {
                                                normal: {
                                                    color: '#EF5350'
                                                }
                                            },
                                            data: [38203, 73489, 129034, 204970, 331744]
                                        },
                                        {
                                            name: 'Year 2014',
                                            type: 'bar',
                                            itemStyle: {
                                                normal: {
                                                    color: '#66BB6A'
                                                }
                                            },
                                            data: [39325, 83438, 131000, 221594, 334141]
                                        }
                                    ]
                                }
                            </script>
                            <%- CHART.bar("bar", scope, {}) %>


                        </div>
                    </div>

                </div>
                <div class="col-sm-12 col-md-12" ng-show="charts.allow(['Stacked'])">
