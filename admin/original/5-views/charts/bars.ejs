                                        },
                                        {
                                            name: 'Chrome',
                                            type: 'bar',
                                            stack: 'Total',
                                            data: [820, 1632, 1901, 2234, 1290, 1330, 1320]
                                        }
                                    ]
                                }


                            </script>
                            <%- CHART.columns("columnsstack", scope, {}) %>
                            <script>
                                charts.charts.bar.refresh();
                            </script>

                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div><!--end-->

