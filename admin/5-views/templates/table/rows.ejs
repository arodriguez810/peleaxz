<td ng-repeat="(key, value) in <%= scope %>.columns()"
    ng-show="value.visible!=false"
    class="{{::value.class || ''}} context-control"
    style="{{::value.style || ''}}"
    data-column="{{::key}}"
    ng-click="<%= scope %>.cell.select($event,row)"
    ng-dblclick="<%= scope %>.cell.dblselect($event)"
>

    <span
            ng-show="<%= scope %>.rowDeleted(row)===''"
            ng-bind-html="<%= scope %>.drawCheck(key,row)"
            ng-click="<%= scope %>.check(row)"
    >
    </span>
    <span ng-bind-html="<%= scope %>.cellValue(key,value,row)"
          ng-click="<%= scope %>.cell.click(key,value,row)"
          ng-dblclick="<%= scope %>.cell.dblclick(key,value,row)"
          ng-mousedown="<%= scope %>.cell.mousedown(key,value,row)"
          ng-mouseenter="<%= scope %>.cell.mouseenter(key,value,row)"
          ng-mouseleave="<%= scope %>.cell.mouseleave(key,value,row)"
          ng-mousemove="<%= scope %>.cell.mousemove(key,value,row)"
          ng-mouseover="<%= scope %>.cell.mouseover(key,value,row)"
          ng-mouseup="<%= scope %>.cell.mouseup(key,value,row)"
    ></span>
</td>